{
    "compilerOptions": {
        "target": "ES2021",
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": false,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "downlevelIteration": true,
        "outDir": "./dist",
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "noErrorTruncation": true
    },
    "include": ["src/**/*", "index.d.ts"],
    "exclude": []
}
