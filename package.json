{
	"name": "@nuggxyz/dotnugg-sdk",
	"version": "0.4.5",
	"description": "sdk for working with dotnugg files written in typescript",
	"license": "BUSL-1.1",
	"repository": "https://github.com/nuggxyz/dotnugg-sdk.git",
	"author": "dub6ix <dub6ix@nugg.xyz>",
	"main": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"files": [
		"dist",
		"LICENSE"
	],
	"scripts": {
		"build:clean": "rm -rf build && yarn build",
		"build": "yarn install && yarn typechain && tsc",
		"start": "ts-node /src/index.ts --input ../nuggft-art --output ./out --filename txs",
		"generate-uploader": "tsc && node ./dist/scripts/unsafeStoreFilesBulkData.js",
		"encode": "ts-node ./src/scripts/encode.ts",
		"typechain": "typechain --target ethers-v5 --out-dir src/typechain/ './src/abis/*'",
		"test": "mocha -r ts-node/esm  src/_tests/**/*.spec.ts",
		"webpack-dev": "webpack --mode development",
		"webpack-watch": "webpack --mode development --watch",
		"package": "yarn typechain && yarn webpack --optimization-minimize --mode production",
		"version:tag": "yarn changeset tag",
		"version:get": "grep '\"version\"' package.json | sed 's/.*\"version\": \"\\(.*\\)\".*/\\1/'",
		"version:commit": "git add . && git commit -m \"v$(yarn version:get)\"",
		"version:changeset": "yarn changeset add && yarn changeset version",
		"version:platform": "echo \"v$(yarn version:get)\"",
		"release": "yarn version:changeset && yarn version:platform  && yarn version:commit && yarn version:tag"
	},
	"dependencies": {
		"@ethersproject/abi": "^5.6.4",
		"@ethersproject/bytes": "^5.6.1",
		"@ethersproject/providers": "^5.6.8",
		"@nuggxyz/dotnugg-grammar": "^0.3.1",
		"chokidar": "^3.5.3",
		"color-parse": "^1.4.2",
		"ethers": "^5.6.9",
		"fsevents": "^2.3.2",
		"minimist": "^1.2.6",
		"node-fetch": "^3.2.9",
		"node-loader": "^2.0.0",
		"plist": "^3.0.6",
		"tiny-invariant": "^1.2.0",
		"vscode-oniguruma": "^1.6.2",
		"vscode-textmate": "^7.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.18.9",
		"@changesets/cli": "^2.24.1",
		"@typechain/ethers-v5": "^10.1.0",
		"@types/chai": "^4.3.1",
		"@types/fs-extra": "^9.0.13",
		"@types/mocha": "^9.1.1",
		"@types/node": "^18.6.2",
		"@types/plist": "^3.0.2",
		"@types/vscode": "^1.69.0",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"arraybuffer-loader": "^1.0.8",
		"chai": "^4.3.6",
		"eslint": "^8.20.0",
		"eslint-config-universe": "^11.0.0",
		"eslint-plugin-import": "^2.26.0",
		"mocha": "^10.0.0",
		"prettier": "^2.7.1",
		"ts-loader": "^9.3.1",
		"ts-node": "^10.9.1",
		"typechain": "^8.1.0",
		"typescript": "^4.7.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	},
	"packageManager": "yarn@3.2.1"
}
