{
    "name": "@nuggxyz/dotnugg-sdk",
    "version": "0.1.0",
    "description": "sdk for working with dotnugg files written in typescript",
    "license": "MIT",
    "repository": "https://github.com/nuggxyz/dotnugg-sdk.git",
    "author": "dub6ix <dub6ix@nugg.xyz>",
    "private": false,
    "main": "./build/index.js",
    "typings": "./index.d.ts",
    "files": [
        "build",
        "LICENSE",
        "src"
    ],
    "scripts": {
        "build:clean": "rm -rf build && yarn build",
        "build": "yarn install && yarn typechain && tsc",
        "start": "tsc && node ./build/index.js --input ../nuggft-art --output ./out --filename txs",
        "generate-uploader": "tsc && node ./build/scripts/unsafeStoreFilesBulkData.js",
        "encode": "tsc && node ./build/scripts/encode.js",
        "typechain": "typechain --target ethers-v5 --out-dir src/typechain/ './src/abis/*'"
    },
    "dependencies": {
        "@ethersproject/abi": "^5.0.0",
        "@ethersproject/bytes": "^5.0.0",
        "@ethersproject/providers": "^5.0.0",
        "@nuggxyz/dotnugg-grammar": "^0.1.5",
        "ethers": "^5.5.4",
        "minimist": "^1.2.5",
        "tiny-invariant": "^1.2.0",
        "vscode-oniguruma": "^1.5.1",
        "vscode-textmate": "^5.4.1"
    },
    "devDependencies": {
        "@babel/core": "^7.16.12",
        "@typechain/ethers-v5": "^9.0.0",
        "@types/fs-extra": "^9.0.13",
        "@types/vscode": "^1.62.0",
        "@typescript-eslint/eslint-plugin": "^5.3.1",
        "@typescript-eslint/parser": "^5.3.1",
        "eslint": "^8.2.0",
        "eslint-config-universe": "^10.0.0",
        "eslint-plugin-import": "^2.25.3",
        "prettier": "^2.4.1",
        "typechain": "^7.0.0",
        "typescript": "^4.4.4"
    }
}
